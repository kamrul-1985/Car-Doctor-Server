{
      "version": 2,
      "builds": [
            { 
                  "src": "index.js", 
                  "use": "@vercel/node-server" 
                  }
            ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "index.js",
          "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
        }
      ]
      
    }